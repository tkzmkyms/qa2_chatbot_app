#!/usr/bin/env ruby
#
# This file was generated by RubyGems.
#
# The application 'tailwindcss-ruby' is installed as part of a gem, and
# this file is here to facilitate running it.
#

require 'rubygems'

Gem.use_gemdeps

version = ">= 0.a"

str = ARGV.first
if str
  str = str.b[/\A_(.*)_\z/, 1]
  if str and Gem::Version.correct?(str)
    version = str
    ARGV.shift
  end
end

if Gem.respond_to?(:activate_bin_path)
load Gem.activate_bin_path('tailwindcss-ruby', 'tailwindcss', version)
else
gem "tailwindcss-ruby", version
load Gem.bin_path("tailwindcss-ruby", "tailwindcss", version)
end
